#include <stdio.h>

// Функція для обчислення кількості рівних дільників числа
int countDivisors(int n) {
    int count = 0;

    // Перебір чисел від 1 до n
    for (int m = 1; m <= n; m++) {
        // Перевірка, чи число m є рівним дільником числа n
        if (n % m == n / m) {
            count++; // Якщо умова виконується, збільшуємо лічильник рівних дільників
        }
    }

    return count; // Повертаємо кількість рівних дільників
}

int main() {
    int n;
    printf("Введіть натуральне число n (1 < n < 150): ");
    scanf("%d", &n);

    // Виклик функції для обчислення кількості рівних дільників
    int result = countDivisors(n);

    // Виведення результату
    printf("Кількість рівних дільників числа %d: %d\n", n, result);

    return 0;
}
