#include <stdio.h>
#include <math.h>

int main() {
    double v, x1, x2, y1, y2;

    // Введення координат точок
    printf("Введіть координати точок (x1, y1, x2, y2): ");
    scanf("%lf %lf %lf %lf", &x1, &y1, &x2, &y2);

    // Обчислення довжини вектора за формулою sqrt((x2 - x1)^2 + (y2 - y1)^2)
    v = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));

    // Виведення довжини вектора з точністю до шести знаків після коми
    printf("Довжина вектора: %.6lf\n", v);

    return 0;
}
