#include <stdio.h>

int main() {
    int n;
    int dp[100000];

    printf("Введіть довжину послідовності n (1 < n < 10000): ");
    scanf("%d", &n);

    dp[1] = 2;    // Початкове значення для довжини послідовності 1
    dp[2] = 4;    // Початкове значення для довжини послідовності 2
    dp[3] = 7;    // Початкове значення для довжини послідовності 3

    for (int i = 4; i <= n; i++) {
        dp[i] = (dp[i - 1] + dp[i - 2] + dp[i - 3]) % 12345;
        // Обчислюю кількість допустимих послідовностей довжиною i
        // на основі попередніх трьох значень
        // і обмежую результат за модулем 12345
    }

    printf("Кількість допустимих послідовностей довжиною %d: %d\n", n, dp[n]);
    // Виводжу кількість допустимих послідовностей для заданої довжини n

    return 0;
}
